{
    "id" : "debug",
    "version" : "0",
    "elements" : [
        {
            "id" : "inRect",
            "const" : {
                "margin" : "$$1"
            },
            "backupProperties" : {
                "$$1" : "0"
            },
            "w" : "rect.w",
            "h" : "rect.h",
            "atoms" : [
                {
                    "id" : "content",
                    "x" : "margin",
                    "y" : "margin",
                    "w" : "$1.w",
                    "h" : "$1.h",
                    "svg" : "<<<$1.svg>>>"
                },
                {
                    "id" : "rect",
                    "w" : "content.w + 2*margin",
                    "h" : "content.h + 2*margin",
                    "svg" : "<rect x=\"0\" y=\"0\" width=\"<<<rect.w>>>\" height=\"<<<rect.h>>>\" style=\"fill:blue;stroke:pink;stroke-width:5;fill-opacity:0.1;stroke-opacity:0.9\" />"
                }
            ]
        }
    ]

}
