{
    "id": "testLab",
    "type": "lab",
    "version": "1.2.3",
    "const": {
    	"defHeight":"25"
    },
    "elements": [
        {
            "type": "atom",
            "id": "notSoSimpleAtom",
            "version": "1.2.2",
            "dependsOn": "defHeight",
            "x": "defHeight",
            "y": "defHeight*2",
            "h": "100",
            "w": "100",
            "svg": "SIMPLE\" ATOM"
        },
        {
            "type": "atom",
            "id": "simpleAtom",
            "version": "1.2.2",
            "x": "0",
            "y": "0",
            "h": "100",
            "w": "100",
            "svg": "SIMPLE\" ATOM"
        },
        {
        	"type": "compound",
            "id": "simpleCompound",
            "version": "1.2.3",
            "x": "1",
            "y": "2",
            "h": "300",
            "w": "400",
            "elements": [
            	{
            		"type": "atom",
            		"id": "subAtom1",
            		"version": "1.2.2",
            		"x": "1",
            		"y": "1",
            		"h": "100",
            		"w": "100",
            		"svg": "content Atom 1"
        		},
        		{
            		"type": "atom",
            		"id": "subAtom2",
            		"version": "1.2.2",
            		"x": "2",
            		"y": "2",
            		"h": "200",
            		"w": "200",
            		"svg": "content Atom 2"
        		}
            ]
        },
        {
            "type": "compound",
            "id": "root",
            "version": "1.2.3",
            "x": "$1.x+2",
            "y": "0",
            "h": "radical.y+radical.h",
            "w": "radix.x+radix.w",
            "const": {
                "aX": "20",
                "aY": "30"
            },
            "elements": [
                {
                    "type": "atom",
                    "id": "power",
                    "dependsOn": "$1",
                    "x": "0",
                    "y": "0",
                    "h": "$1.h",
                    "w": "$1.w",
                    "svg": "<<< $1.svg >>>"
                },
                {
                    "type": "atom",
                    "id": "radical",
                    "dependsOn": "power, radix, base, aX, aY",
                    "x": "power.x+power.w-aX",
                    "y": "power.y+power.h-aY",
                    "h": "radix.h+base.h+5",
                    "w": "35",
                    "svg": "RADICAL RADICAL RADICAL"
                },
                {
                    "type": "atom",
                    "id": "radix",
                    "dependsOn": "radical, base",
                    "x": "radical.x+radical.w",
                    "y": "radical.y",
                    "h": "23",
                    "w": "base.w + 5",
                    "svg": "RADIX RADIX RADIX"
                },
                {
                    "type": "atom",
                    "id": "base",
                    "dependsOn": "radical, radix, $2",
                    "x": "radical.x+radical.w",
                    "y": "radix.y+radix.h",
                    "h": "$2.h",
                    "w": "$2.w",
                    "svg": "AAA <<< $2.svg >>> BBB"
                }
            ]
        }
    ]
}
